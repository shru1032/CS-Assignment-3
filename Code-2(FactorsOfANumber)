//Program to find all the factors of a number

#include <stdio.h>

void main()
{
    int n,i;
    
    printf("Enter a number to find factors:");
    scanf("%d",&n);
    
    printf("The factors of %d are:\n",n);
    for(i=1;i<=n/2;i++)
    {
        if(n%i==0)
        {
            printf("%d\t",i);
        }
    }
}
