//Program to enter week number and print day of week.

#include <stdio.h>

void main()
{
    int weeknum;

    /* Input week number from user */
    printf("Enter week number (1-7): ");
    scanf("%d", &weeknum);


    if(weeknum == 1)
    {
        printf("Monday");
    }
    else if(weeknum == 2)
    {
        printf("Tuesday");
    }
    else if(weeknum == 3)
    {
        printf("Wednesday");
    }
    else if(weeknum == 4)
    {
        printf("Thursday");
    }
    else if(weeknum == 5)
    {
        printf("Friday");
    }
    else if(weeknum == 6)
    {
        printf("Saturday");
    }
    else if(weeknum == 7)
    {
        printf("Sunday");
    }
    else
    {
        printf("Invalid Input! Please enter week number between 1-7.");
    }
}
